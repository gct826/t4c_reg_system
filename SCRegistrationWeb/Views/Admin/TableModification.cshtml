                <div class="divider1"></div>
                <h3>Fellowship Table</h3>
                @{Html.RenderAction("Index", "AdminFellowship");}
            </div>
            <div class="row">
                <div class="divider1"></div>

                <h3>Pricing Table</h3>
                @{Html.RenderAction("Index", "AdminPricing");}
