            <div class="row">
                <div class="divider1"></div>

                <h3>Age Range Table</h3>
                @{Html.RenderAction("Index", "AdminAgeRange");}
            </div>
            <div class="row">
                <div class="divider1"></div>

                <h3>Congregation Table</h3>
                @{Html.RenderAction("Index", "AdminService");}
            </div>
            <div class="row">
